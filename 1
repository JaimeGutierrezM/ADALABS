#include <iostream>
using namespace std;

  int vector[100];

  int count = 0;

  void insert(int element)
  {
  
    if( count < 100)
    {
     vector[count++] = element; 
    }
  
    
  };





int main() {


  



  for(int i = 0; i < 101; i++)
    {
      insert(i);
    }


  for(int i = 0; i < 101; i++)
    {
      cout << vector[i] << "-";
    }

  
  




  
}
